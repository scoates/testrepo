#!/bin/bash

if [ -f $CODEBUILD_SRC_DIR/system/build/$1 ]; then
    source $CODEBUILD_SRC_DIR/system/build/codebuild_extras

    set -x
    if [[ "$1" != "finalizing" ]]; then
        $CODEBUILD_SRC_DIR/system/ircmsg Starting phase $1
    fi
    source $CODEBUILD_SRC_DIR/system/build/$1  # always wrap these with a shell script

fi
